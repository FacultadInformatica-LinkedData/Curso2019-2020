        rdfs:domain    <http://madridcen.es/group30/ontologia#CentroDeportivo>;
        rdfs:range xsd:int .		
		
#  http://madridcen.es/group30/ontologia#codigoPostal
:codigoPostal rdf:type owl:DatatypeProperty ;
